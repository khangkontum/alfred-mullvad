				<key>script</key>
				<string>#!/bin/bash

# Check current Mullvad status
status=$(mullvad status)

if echo "$status" | grep -q "Connected"; then
    # Currently connected, so disconnect
    mullvad disconnect
    echo "dis"
else
    # Currently disconnected, so connect
    mullvad connect
    echo "conn"
fi</string>
